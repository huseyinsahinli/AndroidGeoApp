            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/rectangleFragment"
        android:name="com.hsynshnl.geometriccalculation.RectangleFragment"
        android:label="@string/Rectangle"
        tools:layout="@layout/fragment_rectangle" >
        <action
            android:id="@+id/action_rectangleFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/sphereFragment"
        android:name="com.hsynshnl.geometriccalculation.SphereFragment"
        android:label="@string/Sphere"
        tools:layout="@layout/fragment_sphere">
        <action
            android:id="@+id/action_sphereFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/squareFragment"
        android:name="com.hsynshnl.geometriccalculation.SquareFragment"
        android:label="@string/Square"
        tools:layout="@layout/fragment_square">
        <action
            android:id="@+id/action_squareFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
</navigation>