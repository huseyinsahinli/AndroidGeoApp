<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.hsynshnl.geometriccalculation.MainFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_nav_triangle"
            app:destination="@id/nav_triangle"
            />
        <action
            android:id="@+id/action_mainFragment_to_circleFragment"
            app:destination="@id/circleFragment" />
        <action
            android:id="@+id/action_mainFragment_to_cubeFragment"
            app:destination="@id/cubeFragment" />
        <action
            android:id="@+id/action_mainFragment_to_cylinderFragment"
            app:destination="@id/cylinderFragment" />
        <action
            android:id="@+id/action_mainFragment_to_ellipseFragment"
            app:destination="@id/ellipseFragment" />
        <action
            android:id="@+id/action_mainFragment_to_parallelFragment"
            app:destination="@id/parallelFragment" />
        <action
            android:id="@+id/action_mainFragment_to_sphereFragment"
            app:destination="@id/sphereFragment" />
        <action
            android:id="@+id/action_mainFragment_to_squareFragment"
            app:destination="@id/squareFragment" />
        <action
            android:id="@+id/action_mainFragment_to_quadrilateralFragment"
            app:destination="@id/quadrilateralFragment" />
        <action
            android:id="@+id/action_mainFragment_to_quadrangleFragment"
            app:destination="@id/quadrangleFragment" />
        <action
            android:id="@+id/action_mainFragment_to_polygonFragment"
            app:destination="@id/polygonFragment" />
        <action
            android:id="@+id/action_mainFragment_to_parallelogramFragment"
            app:destination="@id/parallelogramFragment" />
        <action
            android:id="@+id/action_mainFragment_to_trapezoidFragment"
            app:destination="@id/trapezoidFragment" />
        <action
            android:id="@+id/action_mainFragment_to_rectangleFragment"
            app:destination="@id/rectangleFragment" />
    </fragment>
    <include app:graph="@navigation/nav_triangle" />
    <fragment
        android:id="@+id/circleFragment"
        android:name="com.hsynshnl.geometriccalculation.CircleFragment"
        android:label="@string/Circle"
        tools:layout="@layout/fragment_circle">
        <action
            android:id="@+id/action_circleFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/cubeFragment"
        android:name="com.hsynshnl.geometriccalculation.CubeFragment"
        android:label="@string/Cube"
